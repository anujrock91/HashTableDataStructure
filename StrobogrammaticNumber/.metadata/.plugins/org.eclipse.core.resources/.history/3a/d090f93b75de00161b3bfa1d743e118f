package Snippet;

public class StroboGrammaticNum {
	
	public boolean isSGN(int num){
		String s = ""+num;
		int left=0;
		int right=s.length()-1;
		while(left<right){
			int num1 = s.charAt(left)-'0';
			int num2 = s.charAt(right)-'0';
			if(num1!=0 || num1!=1 || num1!=6 || num1!=8 || num1!=9 )
		}
	}
}
